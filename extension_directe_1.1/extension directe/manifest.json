{
    "name": "Extension Directe",
    "description": "Donne les moyennes sur ED",
    "version": "1.1",
    "manifest_version": 3,
    "action":{
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/images/icon_16.png",
            "32": "/images/icon_32.png",
            "48": "/images/icon_48.png",
            "128": "/images/icon_128.png"
          }
    },
    "permissions":["scripting"],
    "host_permissions": ["*://*/"]
  }